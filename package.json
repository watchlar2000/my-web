{
  "name": "my-site",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "eleventy",
    "prepare": "husky",
    "lint": "eslint \"**/*.{js,mjs,cjs,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,mjs,cjs,ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,mjs,cjs,ts,tsx,json,md,yml,yaml,css,html,njk}\""
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,tsx}": [
      "eslint --fix",
      "eslint --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,html,njk}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "Sergiuzh Kochetov",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-watchlar2000-js": "^1.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/font-awesome": "1.0.0-beta.1"
  }
}
